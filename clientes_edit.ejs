<%- include('header.ejs') %>

    <div class="container mt-5">
        <div class="row">
            <div class="col-md-5 offset-md-3 card">
                <div class="card-body">
                    <form action="/update/<%= data.telefono%>" method="POST">
                        <div class="form-group">
                            <input type="text" name="telefono" value="<%=data.telefono%>" class="form-control"
                                autofocus>
                        </div>
                        <div class="form-group">
                            <input type="text" name="nombre" value="<%= data.nombre%>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="direccion" value="<%= data.direccion%>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="rfc" value="<%= data.rfc%>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="correo" value="<%= data.correo%>" class="form-control">
                        </div>
                        <button type="submit" class="btn btn-primary">
                            guardar
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <%- include('footer.ejs') %>